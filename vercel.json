{
  "version": 2,
  "builds": [
    {
      "src": "*.php",
      "use": "@vercel/php"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*).php",
      "dest": "/$1.php"
    },
    {
      "src": "/(.*).html",
      "dest": "/$1.html"
    }
  ]
}
